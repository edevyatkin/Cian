<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta name="format-detection" content="telephone=no" />
    <meta name="format-detection" content="date=no" />
    <meta name="format-detection" content="email=no" />
    <title>@ViewData["Title"]</title>
    <link href="https://fonts.googleapis.com/css?family=PT+Serif" rel="stylesheet">
    <environment names="Development">
        <script src="~/lib/angular/angular.js"></script>
        <script src="~/lib/angular-lazy-img/release/angular-lazy-img.js"></script>
    </environment>
    <environment names="Production">
        <script src="~/lib/angular/angular.min.js"></script>
        <script src="~/lib/angular-lazy-img/release/angular-lazy-img.min.js"></script>
    </environment>
    <script src="~/lib/angular-i18n/angular-locale_ru-ru.js"></script>
    <script src="~/js/flats.js"></script>
</head>
<body>
    @RenderBody()
</body>
</html>
